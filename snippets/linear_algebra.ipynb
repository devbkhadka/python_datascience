   "source": [
    "import numpy as np\n",
    "from numpy import linalg\n",
    "\n",
    "m1 = np.random.randn(3,5)\n",
    "print(m1)\n",
    "m2 = np.eye(5)\n",
    "print(m2)\n",
    "print('m1.dot(m2)\\n', m1.dot(m2))\n",
    "print('m1@m2\\n', m1@m2)\n"
   ]
  },
